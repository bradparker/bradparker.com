cabal-version: 2.4
name: bradparker-com-server
version: 0.1.0.0
license: BSD-3-Clause
build-type: Simple

executable server
  main-is:
    Main.hs
  build-depends:
      base == 4.*
    , optparse-applicative
    , wai
    , wai-app-static
    , warp
  ghc-options:
    -threaded -Wall
  default-language:
    Haskell2010
